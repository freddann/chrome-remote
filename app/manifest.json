{
    "manifest_version": 2,
    "name": "ChromeControl App",
    "description": "Relays messages to extension.",
    "version": "2",
    "minimum_chrome_version": "23",
    "sockets":{
        "tcpServer": {
            "listen": "*"
        }
    },
    "app": {
        "background": {
            "scripts": [
                "lib/underscore.min.js",
                "lib/wsc-chrome.min.js",
                "http.js"
            ]
        }
    }
}